{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:vue/recommended", "plugin:prettier/recommended", "prettier/vue"],
  "rules": {
    "vue/component-name-in-template-casing": ["error", "PascalCase"],
    "no-console": "error",
    "no-debugger": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  }
}
